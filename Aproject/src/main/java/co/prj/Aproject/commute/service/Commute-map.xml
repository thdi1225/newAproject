<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="co.prj.Aproject.commute.service.CommuteMapper">

	<select id="commuteSelectList" resultType="co.prj.Aproject.commute.vo.CommuteVO">
		
		select* from commute order by 1
	
	</select>
	
	<select id="commuteStartTimeSelect" resultType="co.prj.Aproject.commute.vo.CommuteVO">
	
		SELECT TO_CHAR(COMMUTE_START_TIME,'""HH24"시 "MI"분 "SS"초"') AS COMMUTE_START_TIME
		FROM COMMUTE
		WHERE COMMUTE_ID = #{commute_Id}
	
	</select>
	
	<select id="commuteEndTimeSelect" resultType="co.prj.Aproject.commute.vo.CommuteVO">
	
		SELECT TO_CHAR(COMMUTE_END_TIME,'""HH24"시 "MI"분 "SS"초"') AS COMMUTE_END_TIME
		FROM COMMUTE
		WHERE COMMUTE_ID = #{commute_Id}
	
	</select>

	<insert id="commuteStartInsert" >
	
		    INSERT INTO COMMUTE(COMMUTE_ID,COMMUTE_DATE,COMMUTE_START_TIME)
		VALUES(COMMUTE_SEQ.NEXTVAL,SYSDATE,SYSDATE)
		
	</insert>
	
	
	<insert id="commuteEndInsert" parameterType="co.prj.Aproject.commute.vo.CommuteVO">
	
		    INSERT INTO COMMUTE(COMMUTE_ID,COMMUTE_DATE,COMMUTE_End_TIME)
		VALUES(COMMUTE_SEQ.NEXTVAL,SYSDATE,SYSDATE)
	 
	</insert>
	

</mapper>